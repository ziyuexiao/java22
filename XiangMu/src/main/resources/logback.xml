<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
    <appender name="fileout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="trace">
        <appender-ref ref="fileout"/>
    </root>
</configuration>